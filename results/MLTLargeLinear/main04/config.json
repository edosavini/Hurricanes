{
    "dataset_reader": {
        "type": "hur_reader0",
        "bert_model_name": "bert-large-uncased",
        "token_indexers": {
            "bert": {
                "type": "bert-pretrained",
                "pretrained_model": "bert-large-uncased"
            }
        }
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 16,
        "sorting_keys": [
            [
                "quote_response",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "emotion_classifier_mlt",
        "bert_model_name": "bert-large-uncased",
        "report_auxiliary_metrics": false
    },
    "train_data_path": "data/Train4.json",
    "validation_data_path": "data/Val4.json",
    "test_data_path": "data/Test4.json",
    "trainer": {
        "cuda_device": 1,
        "grad_clipping": 5,
        "num_epochs": 30,
        "optimizer": {
            "type": "adagrad",
            "lr": 2e-05
        },
        "patience": 3,
        "validation_metric": "+average_F1"
    },
    "dataset_reader_aux": {
        "type": "hur_reader_aux0",
        "bert_model_name": "bert-large-uncased",
        "token_indexers": {
            "bert": {
                "type": "bert-pretrained",
                "pretrained_model": "bert-large-uncased"
            }
        }
    },
    "evaluate_on_test": true,
    "iterator_aux": {
        "type": "bucket",
        "batch_size": 16,
        "sorting_keys": [
            [
                "quote_response",
                "num_tokens"
            ]
        ]
    },
    "mixing_ratio": 0.06,
    "train_data_path_aux": "data/Train4.json"
}